{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Verbose"
        }
      },
      {
        "Name": "File",
        "Args": {
          "rollingInterval": "Day",
          "path": "Logs/Errors/log.txt",
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "rollingInterval": "Day",
          "path": "Logs/Traces/log.txt",
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //"DefaultConnection": "Server=localhost\\SQLEXPRESS01; Database=SatelliteDatabase;Trusted_Connection=True;TrustServerCertificate=True;"
    "DefaultConnection": "Server=192.168.1.23,1433;Initial Catalog=ViktorOnboarding;User Id=Viktor;Password=pass;TrustServerCertificate=True;"

